   "source": [
    "# IsoMap (多様体学習の一手法； 次元を削減し、データの構造を明らかにする)\n",
    "from sklearn.manifold import Isomap\n",
    "iso = Isomap(n_components=2)\n",
    "projection = iso.fit_transform(digits.data)\n",
    "\n",
    "# 結果の描画\n",
    "plt.scatter(projection[:, 0], projection[:, 1], lw=0.1,\n",
    "           c='black')"
   ]
  },
